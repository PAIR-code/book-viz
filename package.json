{
  "name": "book-viz",
  "version": "1.0.0",
  "description": "visualizing books with sentence embeddings",
  "main": "dist/bundle.js",
  "scripts": {
    "prestart": "tsc && rollup -c",
    "serve": "live-server --watch=*.html,dist/bundle.js,css/*.css",
    "tsc:w": "tsc -w",
    "rollup:w": "rollup -c -w",
    "start": "npm-run-all --parallel serve tsc:w rollup:w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PAIR-code/book-viz"
  },
  "author": "hsuresh",
  "license": "ISC",
  "devDependencies": {
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.34.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-watch": "^4.3.1",
    "tslint": "^5.20.1"
  },
  "dependencies": {
    "@types/d3": "^5.16.4",
    "@types/d3-array": "^2.8.0",
    "@types/dom-inputevent": "^1.0.6",
    "@types/node": "^14.14.12",
    "d3": "^5.16.0",
    "getmdl-select": "^2.0.1",
    "jquery": "^3.5.1",
    "material-design-lite": "^1.3.0",
    "typescript": "^4.1.2"
  }
}
